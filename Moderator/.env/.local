# Meta
USE_DOCKER=yes
IPYTHONDIR=/app/.ipython

# Yearbook Env
Yearbook_ENV=local

# AWS Credentials
AWS_ACCESS_KEY_ID=<YourAccessKeyId>
AWS_SECRET_ACCESS_KEY=<YourSecretAccessKey>

# Celery and Celery Broker (Redis) Settings
CELERY_BROKER=REDIS
REDIS_USER=default
REDIS_PASSWORD=<RedisPassword>
REDIS_SCHEME=redis
REDIS_PORT=6379
REDIS_SUFFIX=0
CELERY_HOST_CONTAINER=cache


CELERY_BROKER_NAME=${CELERY_BROKER_SCHEME}://${CELERY_BROKER_USER}:${CELERY_BROKER_PASSWORD}@${CELERY_BROKER_HOST}:${CELERY_BROKER_PORT}/${CELERY_BROKER_VHOST}
CELERY_BROKER_PASSWORD=<RedisPassword>
CELERY_BROKER_USER=default
CELERY_BROKER_SCHEME=redis
CELERY_BROKER_PORT=6379
CELERY_BROKER_HOST=cache
CELERY_BROKER_VHOST=0
CELERY_BROKER_URL=${CELERY_BROKER_SCHEME}://${CELERY_BROKER_USER}:${CELERY_BROKER_PASSWORD}@${CELERY_BROKER_HOST}:${CELERY_BROKER_PORT}/${CELERY_BROKER_VHOST}
CELERY_BROKER_HOSTNAME=${CELERY_BROKER_URL}
CELERY_BROKER_WRITE_URL=${CELERY_BROKER_URL}
CELERY_RESULT_BACKEND=django-db


# Global Variables
MAX_QUEUE_SIZE=500

# Database Settings
POSTGRES_USER=Yearbookadmin
POSTGRES_PASSWORD=<YourPassword>
POSTGRES_HOST=db
POSTGRES_PORT=5432
POSTGRES_DB=Yearbook_moderator

# Message Queue Setting
MESSAGE_QUEUE_BROKER=RABBITMQ
MESSAGE_QUEUE_SCHEME=amqp
MESSAGE_QUEUE_HOST=mq
RABBITMQ_PORT=5672
RABBITMQ_USER=Yearbookadmin
RABBITMQ_PASSWORD=<YourPassword>

# Social Service URL
SOCIAL_SERVICE_URL=http://Yearbook-social-django:8000/

# APP Name
APP_NAME=Moderator